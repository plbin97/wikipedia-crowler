                          </mrow>
                          <mn>2</mn>
                        </mfrac>
                      </mrow>
                    </mrow>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mi>p</mi>
                    </mrow>
                  </mroot>
                </mrow>
                <mo stretchy="false">)</mo>
                <msup>
                  <mo stretchy="false">)</mo>
